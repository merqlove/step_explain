{
  "name": "step_explain",
  "version": "1.0.10",
  "description": "Converting StepShot HTML to Dr.Explain compatible format in HTML.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/merqlove/step_explain.git"
  },
  "keywords": [
    "npm",
    "node",
    "stepshot",
    "dr.explain",
    "converter",
    "javascript"
  ],
  "scripts": {
    "build": "./node_modules/.bin/babel ./src -d ./lib",
    "lint": "./node_modules/.bin/eslint ./src",
    "test": "./node_modules/.bin/mocha --require @babel/register",
    "serve": "./lib/cli.js -s ./index.html -d ./tmp"
  },
  "author": "Alexander Merkulov",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.19.0",
    "ejs": "^2.6.1",
    "jsdom": "^13.2.0",
    "ncp": "^2.0.0"
  },
  "bugs": {
    "url": "https://github.com/merqlove/step_explain/issues"
  },
  "homepage": "https://github.com/merqlove/step_explain#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.15.0",
    "mocha": "^5.2.0"
  },
  "bin": {
    "stepexp": "./lib/cli.js"
  }
}
